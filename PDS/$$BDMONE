//SBGOLOBU JOB (CCMVS),ASMLINK,
// NOTIFY=&SYSUID,
// CLASS=B,MSGCLASS=X
//* --------------------------------------------------------------- *//
//* --  ASSEMBLE AND LINKEDIT BROADCAST MANAGER LOAD MODULES     -- *//
//* --     ALL PROGRAMS ARE ASSEMBLED WHOSE SOURCE IS HERE.      -- *//
//* --------------------------------------------------------------- *//
//*
//* NOTE:   INCLUDE=INCLUDEA CREATES AN AC(1) MODULE
//*
//* --------------------------------------------------------------- *//
//*
//ASMLKBDM PROC MBR=,INCLUDE=INCLUDE
//*
//*     ASM LINKEDIT FROM SOURCE ASM
//*
//ASMH EXEC PGM=ASMA90,PARM=(OBJECT,NODECK,NOESD,NORLD,NOXREF,FLAG(5))
//STEPLIB  DD DISP=SHR,DSN=SBGOLOB.R20.HLASM
//SYSLIB   DD DISP=SHR,DSN=SBGOLOB.S19.MACLIB
//         DD DISP=SHR,DSN=SBGOLOB.S19.MODGEN
//         DD DISP=SHR,DSN=SBGOLOB.BROD.ASM                 << LIBRARY
//SYSIN    DD DISP=SHR,DSN=SBGOLOB.BROD.ASM(&MBR)
//SYSUT1   DD UNIT=SYSALLDA,SPACE=(CYL,2)
//SYSPRINT DD SYSOUT=*
//SYSLIN   DD DISP=SHR,DSN=SBGOLOB.BRODMSTR.OBJECT(&MBR)
//*           DSN=&&TEMP
//*
//LKED   EXEC  PGM=HEWL,
//       PARM='NCAL,MAP,LIST,LET,NORENT,NOREUS'
//SYSUT1   DD  UNIT=SYSALLDA,SPACE=(2048,(200,20))
//SYSPRINT DD  SYSOUT=*
//SYSLIB   DD  DISP=SHR,DSN=SBGOLOB.BRODMSTR.LOADLIB
//SYSLMOD  DD  DISP=SHR,DSN=SBGOLOB.BRODMSTR.LOADLIB(&MBR)
//OBJECT   DD  DISP=SHR,DSN=SBGOLOB.BRODMSTR.OBJECT(&MBR)
//SYSLIN   DD  DISP=SHR,DSN=SBGOLOB.BROD.ASM(&INCLUDE)
//*
//   PEND
//*
//*DMCALC  EXEC ASMLKBDM,MBR=BDMCALC
//*DMCLEAN EXEC ASMLKBDM,MBR=BDMCLEAN
//*DMCVMSG EXEC ASMLKBDM,MBR=BDMCVMSG
//*DMDEL   EXEC ASMLKBDM,MBR=BDMDEL
//*DMDIAG  EXEC ASMLKBDM,MBR=BDMDIAG
//BDMDSFIX EXEC ASMLKBDM,MBR=BDMDSFIX
//*DMDUMP  EXEC ASMLKBDM,MBR=BDMDUMP
//*DMEXPND EXEC ASMLKBDM,MBR=BDMEXPND
//*DMGTMSG EXEC ASMLKBDM,MBR=BDMGTMSG
//*DMGTUID EXEC ASMLKBDM,MBR=BDMGTUID
//*DMINIT  EXEC ASMLKBDM,MBR=BDMINIT
//*DMLIST  EXEC ASMLKBDM,MBR=BDMLIST
//*DMLRBA  EXEC ASMLKBDM,MBR=BDMLRBA
//*DMMKBAK EXEC ASMLKBDM,MBR=BDMMKBAK
//*DMMLOAD EXEC ASMLKBDM,MBR=BDMMLOAD
//*DMNLIST EXEC ASMLKBDM,MBR=BDMNLIST
//*DMNOTFY EXEC ASMLKBDM,MBR=BDMNOTFY
//*DMNUPD  EXEC ASMLKBDM,MBR=BDMNUPD,INCLUDE=INCLUDEA
//*DMNXPRT EXEC ASMLKBDM,MBR=BDMNXPRT
//*DMREST  EXEC ASMLKBDM,MBR=BDMREST
//*DMSCAN  EXEC ASMLKBDM,MBR=BDMSCAN
//*DMSEND  EXEC ASMLKBDM,MBR=BDMSEND
//*DMULOAD EXEC ASMLKBDM,MBR=BDMULOAD
//*DMUSADD EXEC ASMLKBDM,MBR=BDMUSADD
//*DMUSDEL EXEC ASMLKBDM,MBR=BDMUSDEL
//*DMUSERS EXEC ASMLKBDM,MBR=BDMUSERS
