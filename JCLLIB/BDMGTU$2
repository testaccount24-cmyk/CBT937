//            DSN=SBGOLOB.NEW.USEROUT,
//            DCB=(LRECL=8,BLKSIZE=8000,DSORG=PS),
//            SPACE=(TRK,(15,15),RLSE),
//            UNIT=SYSALLDA,VOL=SER=WORK05
//EXPORT   DD DISP=(NEW,CATLG,DELETE),     >> GENERATE BDMUSADD CARDS
//            DSN=SBGOLOB.NEW.USEREXP,
//            DCB=(LRECL=80,BLKSIZE=8000,DSORG=PS),
//            SPACE=(TRK,(15,15),RLSE),
//            UNIT=SYSALLDA,VOL=SER=WORK05
//SORTUID  EXEC PGM=SORT
//SYSOUT   DD SYSOUT=*
//SYSPRINT DD SYSOUT=*
//SORTWK01 DD UNIT=SYSALLDA,SPACE=(TRK,(30,15))
//SORTIN   DD DSN=SBGOLOB.NEW.USEROUT,DISP=SHR
//SORTOUT  DD DSN=SBGOLOB.NEW.SORTOUT,DISP=(,CATLG,DELETE),
