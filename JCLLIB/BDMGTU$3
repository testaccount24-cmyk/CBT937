//SBGOLOBG JOB ,'SAM GOLOB',CLASS=B,
//    MSGCLASS=X,NOTIFY=&SYSUID
//*
//*-----------------------------------------------------------------*//
//*   BDMGTUID EXECUTION - GET USERID LIST FROM A BRODCAST DATASET  *//
//*-----------------------------------------------------------------*//
//GETUSERS EXEC PGM=BDMGTUID
//STEPLIB  DD DISP=SHR,DSN=SBGOLOB.BDM150A.LOADLIB
//BRODCAST DD DISP=SHR,DSN=SBGOLOB.NEW.BRODCAST
//SYSPRINT DD SYSOUT=*
//SYSOUT   DD SYSOUT=*
//SYSUDUMP DD SYSOUT=*
//SORTIN   DD  DSN=&&SORT,DISP=(NEW,PASS),UNIT=SYSALLDA,
//             DCB=(RECFM=FB,LRECL=8,BLKSIZE=8000),
//             SPACE=(TRK,(5,5))
//SORTOUT  DD  DSN=&&SORT,DISP=(SHR,PASS),VOL=REF=*.SORTIN
//BDMUSOUT DD DISP=(NEW,CATLG,DELETE),
//            DSN=SBGOLOB.NEW.USEROUX,
//            DCB=(LRECL=8,BLKSIZE=8000,DSORG=PS),
//            SPACE=(TRK,(15,15),RLSE),
//            UNIT=SYSALLDA,VOL=SER=WORK05
//*-------------------------------------------------------------*//
//*   BDMUSORT EXECUTION - SORT USERIDS FOR INPUT TO BDMULOAD   *//
//*-------------------------------------------------------------*//
//BDMUSORT EXEC PGM=BDMUSORT,REGION=3000K
//STEPLIB  DD  DISP=SHR,DSN=SBGOLOB.BDM150A.LOADLIB
//USERIDS  DD  DISP=SHR,DSN=SBGOLOB.NEW.USEROUX
//USRIDOUT DD  DISP=(NEW,CATLG,DELETE),DSN=SBGOLOB.NEW.USRSORX,
//             DCB=(RECFM=FB,DSORG=PS,LRECL=8,BLKSIZE=8000),
//             SPACE=(TRK,(5,5)),UNIT=SYSALLDA,VOL=SER=WORK04
//SORTIN   DD  DSN=&&SORT,DISP=(NEW,PASS),UNIT=SYSALLDA,
//             DCB=(RECFM=FB,LRECL=8,BLKSIZE=8000),
//             SPACE=(TRK,(5,5))
//SORTOUT  DD  DSN=&&SORT,DISP=(SHR,PASS),VOL=REF=*.SORTIN
//SYSOUT   DD  SYSOUT=*
//SYSPRINT DD  SYSOUT=*
